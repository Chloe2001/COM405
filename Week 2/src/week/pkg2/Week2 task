}

for (int timesTable=2; timesTable <=2;timesTable++)
{
    System.out.println("printing the " +timesTable +"times table");
    for(int i =0 ; i<=10; i++)
    { 
        System.out.println(i*timesTable);
    }
}
int plus = 5;
do 
        {
